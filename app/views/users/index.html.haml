= render 'left_menu'
= link_to 'View My Profile', user_profile_path(user_id)
%h1 Listing users

%table.table
  %tr
    %th Email
  - @users.each do |user|

    %tr
    %td= user.email
    %td= link_to 'Edit', edit_user_path(user)

    %td= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' }

%br
