= render 'left_menu'
%h1 Listing users

%table.table
  %tr
    %th Email
  - @users.each do |user|

    %tr
    %td= user.email
    %td= link_to 'Edit', edit_user_path(user)
    %td= link_to 'CreateProfile', ':current_user/createProfile', :remote => true
    %td= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' }

%br

